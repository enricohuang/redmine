<%= title l(:label_label_new) %>

<%= labelled_form_for @label, :url => project_labels_path(@project) do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag l(:button_create) %>
  <%= link_to l(:button_cancel), settings_project_path(@project, :tab => 'labels') %>
<% end %>
